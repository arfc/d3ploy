<simulation>
  <archetypes>
    <spec>
      <lib>d3ploy.demand_fac</lib>
      <name>DemandFac</name>
    </spec>
    <spec>
      <lib>agents</lib>
      <name>NullRegion</name>
    </spec>
    <spec>
      <lib>d3ploy.demand_driven_deployment_inst</lib>
      <name>DemandDrivenDeploymentInst</name>
    </spec>
  </archetypes>
  <control>
    <duration>40</duration>
    <startmonth>1</startmonth>
    <startyear>2000</startyear>
  </control>
  <facility>
    <config>
      <DemandFac>
        <supply_commod>POWER</supply_commod>
        <demand_commod>fuel</demand_commod>
        <demand_rate_max>2000.</demand_rate_max>
        <demand_rate_min>2000.</demand_rate_min>
        <supply_rate_max>1000.</supply_rate_max>
        <supply_rate_min>1000.</supply_rate_min>
      </DemandFac>
    </config>
    <name>Reactor</name>
  </facility>
  <facility>
    <config>
      <DemandFac>
        <supply_commod>fuel</supply_commod>
        <demand_commod>uranium</demand_commod>
        <demand_rate_max>200000.</demand_rate_max>
        <demand_rate_min>200000.</demand_rate_min>
        <supply_rate_max>10000.</supply_rate_max>
        <supply_rate_min>10000.</supply_rate_min>
      </DemandFac>
    </config>
    <name>Enrichment</name>
  </facility>
  <facility>
    <config>
      <DemandFac>
        <supply_commod>uranium</supply_commod>
        <demand_commod>x</demand_commod>
        <demand_rate_max>0.</demand_rate_max>
        <demand_rate_min>0.</demand_rate_min>
        <supply_rate_max>100000.</supply_rate_max>
        <supply_rate_min>100000.</supply_rate_min>
      </DemandFac>
    </config>
    <name>Mine</name>
  </facility>
  <region>
    <config>
      <NullRegion>
      </NullRegion>
    </config>
    <institution>
      <config>
        <DemandDrivenDeploymentInst>
	        <calc_method>sw_seasonal</calc_method>
          <demand_eq>300*t</demand_eq>
          <facility_commod>
            <item>
              <facility>Reactor</facility>
              <commod>POWER</commod>
            </item>
            <item>
              <facility>Enrichment</facility>
              <commod>fuel</commod>
            </item>
            <item>
              <facility>Mine</facility>
              <commod>uranium</commod>
            </item>
          </facility_commod>
          <facility_capacity>
            <item>
              <facility>Reactor</facility>
              <capacity>1000</capacity>
            </item>
            <item>
              <facility>Enrichment</facility>
              <capacity>10000</capacity>
            </item>
            <item>
              <facility>Mine</facility>
              <capacity>100000</capacity>
            </item>
          </facility_capacity>
          <record>1</record>
        </DemandDrivenDeploymentInst>
      </config>
      <initialfacilitylist>
        <entry>
          <number>1</number>
          <prototype>Reactor</prototype>
        </entry>
        <entry>
          <number>1</number>
          <prototype>Enrichment</prototype>
        </entry>
        <entry>
          <number>1</number>
          <prototype>Mine</prototype>
        </entry>
      </initialfacilitylist>
      <name>ReactorInst</name>
    </institution>
    <name>SingleRegion</name>
  </region>
</simulation>